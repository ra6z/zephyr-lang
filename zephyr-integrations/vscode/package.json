{
  "name": "zephyr",
  "displayName": "Zephyr",
  "description": "Zephyr Programming Language",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zephyr",
        "aliases": [
          "Zephyr",
          "zephyr"
        ],
        "extensions": [
          ".zph"
        ],
        "icon": {
          "light": "./icons/zephyr_light.svg",
          "dark": "./icons/zephyr_dark.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zephyr",
        "scopeName": "source.zph",
        "path": "./syntaxes/zephyr.tmLanguage.json"
      }
    ],
    "commands": [],
    "snippets": [
      {
        "language": "zephyr",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "vscode-languageclient": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "tslint": "^5.16.0",
    "typescript": "^5.0.4"
  }
}
